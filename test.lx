fun fibonacci(n) {
    if (n <= 1) return n;
    return fibonacci(n - 2) + fibonacci(n - 1);
}

for (var i = 0; i < 20; i = i + 1) {
    var fib = fibonacci(i);
    print(fib);
    if (fib == 610) {
        print("610 is a fibonacci number!");
        break;
    }
}


fun thrice(fn) {
    for (var i = 1; i <= 3; i = i + 1) {
        fn(i);
    }
}

thrice(fun (a) {
    print("lambda result " + a);
});